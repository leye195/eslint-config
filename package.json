{
  "name": "eslint-config",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:eslint-base": "pnpm --filter ./packages/eslint-base build",
    "build:eslint-typescript": "pnpm --filter ./packages/eslint-typescript build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""

  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.6",
    "typescript": "^5.4.5",
    "eslint": "^8.48.0"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  }
}
